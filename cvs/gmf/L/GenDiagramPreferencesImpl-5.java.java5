                return isShowConnectionHandles () ? Boolean.TRUE : Boolean.FALSE;
            case GMFGenPackage.GEN_DIAGRAM_PREFERENCES__SHOW_POPUP_BARS :
                return isShowPopupBars () ? Boolean.TRUE : Boolean.FALSE;
